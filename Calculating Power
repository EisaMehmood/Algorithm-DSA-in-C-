// Calculating Power
#include<iostream>
using namespace std;
int power(int a , int b)
{
    int sum =1;
    
    for(int i =1; i<=b; i++)
    {
        sum = sum * a;
    }
    
    return sum;
}
int main()
{
    int a; 
    int b;
    cout<<"Enter Value : "<<endl;
    cin>>a;
    cout<<"Enter power for the Value : "<<endl;
    cin>>b;
    cout<<power(a,b);
    
    
    return 0;
}
