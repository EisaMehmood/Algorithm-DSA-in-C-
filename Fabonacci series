// Fabonacci series
#include <iostream>
using namespace std;
int fab(int n ){
  int a = 0;
  int b = 1;
  int c =0;
  cout<<a<< " "<<b <<" ";
  for(int i =1; i<10; i++)
  {
     c =a+b;
      a=b;
      b=c;
     cout<<c<<" ";
  }

}

int main() {
    // Write C++ code here
  
    fab();

    return 0;
}
