// Check palindrome
#include<iostream>
#include<string>
using namespace std;
int check (string s )
{
    int i =0; 
    int j = s.length()-1;
    while(i<j)
    {
        if(s[i]==s[j])
        {
            return 1;
        }
        
         i++;
         j--;
    }
    return -1;
}
int main()
{
  string s = "cabbbbac";
  cout<<check (s);
  
  
 return 0;
}
