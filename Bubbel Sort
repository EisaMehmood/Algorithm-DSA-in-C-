// Bubble sorting
#include <iostream>
using namespace std;
void bs(int * arr , int size)
{   
    
    for(int i =0 ; i< size-1;i++)
    {
        for(int j = 0; j<size-1; j++)
        {
        // go to 0 to 6
        if(arr[j]>arr[j+1])
        {
            swap(arr[j],arr[j+1]);
             
        }
        
        }
    }
     
      for(int i =0; i<=size-1;i++)
      {
          cout<<arr[i]<<" ";
      }
}

int main() {
     int arr[] = {64, 34, 25, 12, 22, 11, 90 };
       bs(arr,7);

    return 0;
}
